    "@mui/x-date-pickers": "^8.1.0",
    "aws-amplify": "^6.14.1",
    "dayjs": "^1.11.13",
    "moment": "^2.30.1",
    "@types/web-push": "^3.6.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "web-push": "^3.6.7"
